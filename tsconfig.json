	"include": ["src/**/*.ts", "src/containers/Veriff/Session/index.tsx", "src/components/view/Veriff/index.tsx", "src/components/view/index.tsx", "src/components/view/index.tsx", "src/pages/Veriff/Session/index.tsx", "src/pages/Veriff/index.tsx", "src/containers/Profile/BusinessProfile/index.tsx", "src/components/view/Profile/BusinessProfile/UploadDocuments/index.tsx"],
